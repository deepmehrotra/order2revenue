<script type="text/javascript">
$(document).ready(function () {
    var winHeight = $(window).height()+18;
    $('#inSlider.carousel, .header-back, #inSlider.carousel .item').css('height', winHeight)

    $('.tabs-head li > a').click(function(e){
        e.preventDefault();
        var Tabs_index = $('a[id^="tabsLink_"]').index(this);
        $(this).closest('.tabs-head').find('.active').removeClass('active');
        $(this).addClass('active');
        $('.tabsCnt > div').css('display','none')
        $('.tabsCnt > div').eq(Tabs_index).fadeIn();
    });

    $(document).on('click', ".popUp-link", function (e) {
        e.preventDefault();
        $("#hover").show();
        $("#popup").fadeIn();
    });

    //Popup Script Start
    $(document).on('click', "#close", function (e) {
        e.preventDefault();
        $("#hover").hide();
        $("#popup").hide();
    });
})

function submitQuery(){
	 var validator = $("#contactusform").validate({
		  rules: {
			  email: {
			        required: true,
			            }		            
			  },
	     messages: {
	    	 email: "Email is mandatory",
	        }
	 });
	 if(validator.form())
	 { 
		 $.ajax({
         url: $("#contactusform").attr("action"),
         context: document.body,
         type: 'post',
         data:$("#contactusform").serialize(),
         success : function(res) {
        	 if(res=="true")
        		 alert("Thank you for contacting us. We will get back to you shortly!");
        	   $("#hover").hide();
               $("#popup").hide();
        
     }
  });
	 }
}
</script>
